pipeline {
    agent any
    tools{
        jdk 'myjava'
        maven 'mymaven'
    }
     
        stages {
        stage('compile') {
            steps {
                script{
                    echo "compile the code"
                    sh "mvn compile"
                }
                }
        }    
          stage('UnitTest') {    
           
        
            steps {
                script{
                    echo "Running the test cases"
                    sh "mvn test"
                }
            }
        }


                stage('package') {                    
   
            steps {
                script{
                    echo "package the code"
                    sh "mvn package"
                    
                }
                }
                }
                
            }

            
            }
        

